var el = document.getElementById("<%= "game-" + @game.id.to_s %>");
el.querySelector('button i span').innerHTML = "移出收藏";
el.querySelector('button').classList.remove("btn-danger");
el.querySelector('button').classList.add("btn-outline-primary");
el.href = "<%= unfavorite_game_path(@game) %>"
el.querySelector('button').blur()
if (document.getElementById('fav-count')) {
  document.getElementById('fav-count').innerHTML = '收藏(' + <%= @favorites_count %> + ')'
}
